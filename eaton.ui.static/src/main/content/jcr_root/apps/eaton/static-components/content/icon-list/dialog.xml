<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
  jcr:primaryType="cq:Dialog"
  xtype="dialog">
  <items
    jcr:primaryType="cq:Widget"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
      <tab1
        jcr:primaryType="cq:Panel"
        title="Component Properties - Classic UI"
        xtype="panel">
        <items jcr:primaryType="cq:WidgetCollection">
          <view
            jcr:primaryType="cq:Widget"
            allowBlank="false"
            defaultValue="proof-points"
            fieldDescription="Determine which variation of component to view"
            fieldLabel="View"
            fieldSubLabel=""
            name="./view"
            type="select"
            xtype="selection">
            <options jcr:primaryType="cq:WidgetCollection">
              <proofPoints
                jcr:primaryType="nt:unstructured"
                text="Proof Points"
                value="proof-points"/>
              <noImageHighlights
                jcr:primaryType="nt:unstructured"
                text="No Image Highlights"
                value="no-image-highlights"/>
              <top-list-with-icon
                jcr:primaryType="nt:unstructured"
                text="Defaul (3 column)"
                value="top-list-with-icon"/>
            </options>
          </view>
        </items>
      </tab1>
    </items>
  </items>
</jcr:root>
